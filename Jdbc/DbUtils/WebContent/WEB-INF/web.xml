<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<!-- -->
	<servlet>
		<display-name>InitApplicationServlet</display-name>
		<servlet-name>InitApplicationServlet</servlet-name>
		<servlet-class>com.ctvit.db.servlet.InitApplicationServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>InitApplicationServlet</servlet-name>
		<url-pattern>/InitApplicationServlet</url-pattern>
	</servlet-mapping>
 	
</web-app>